                <stroke android:width="1dp"/>
            </shape>
        </rotate>
    </item>

    <item>
        <rotate android:fromDegrees="75">
            <shape android:shape="line">
                <stroke android:width="1dp"/>
            </shape>
        </rotate>
    </item>

    <item>
        <rotate android:fromDegrees="90">
